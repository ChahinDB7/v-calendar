  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChahinDB7/v-calendar.git"
  },
  "homepage": "https://github.com/ChahinDB7/v-calendar#readme",
  "bugs": {
    "url": "https://github.com/ChahinDB7/v-calendar/issues"
  }
