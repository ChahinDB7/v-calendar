  },
  "types": "dist/v-calendar.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --port 3004",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@popperjs/core": "2.9.0",
    "@types/lodash": "^4.14.165",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "lodash": "^4.17.20",
    "moment": "^2.30.1",
    "vue-screen-utils": "^1.0.0-beta.9"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.15.1",
    "@popperjs/core": "^2.11.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^9.7.8",
    "eslint": "^8.17.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-vue": "^9.1.1",
    "prettier": "^2.5.1",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "typescript": "^4.8.4",
    "vite": "^6.0.1",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vue": "^3.2.0"
  },
  "resolutions": {
    "postcss": "^8.3.11"
